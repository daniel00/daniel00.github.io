{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "permalink",
          "name": "permalink",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "path": "[[workspace]]/_posts/Chrome",
      "title": "Chrome"
    },
    {
      "path": "[[workspace]]/_posts/GitBlog",
      "title": "GitBlog"
    },
    {
      "path": "[[workspace]]/_posts/testdir1"
    }
  ]
}